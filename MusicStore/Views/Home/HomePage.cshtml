@{
    Layout = null;
}

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/HomePageStyle.css">
    <title>Landing Page!</title>
  </head>
  <body>
      
    <div class="navigation">
        <div class="logo-area">
          <img src="https://img.freepik.com/premium-vector/music-icons-neon-musical-instruments-signs_53562-12650.jpg?w=1380" class="logo-area"" height="80" />
        </div>
        <div class="nav-area">
            <ul>
                <li>
                    <a href="/Home/Index/" onmouseenter="changeImg('https://images.pexels.com/photos/1943411/pexels-photo-1943411.jpeg')">User</a>
                </li>
                <li>
                     <a href="/Admin/Index/" onmouseenter="changeImg('https://images.pexels.com/photos/3771097/pexels-photo-3771097.jpeg')">Admin</a>
                </li>
                <li>
                  <a href="/Home/AboutUs/" onmouseenter="changeImg('https://images.pexels.com/photos/3183150/pexels-photo-3183150.jpeg')">About Us</a>
              </li>
            </ul>
        </div>
        <div class="image-area" name="slider" id="slider">
           
        </div>
</div>
    <script>
          function changeImg(imgchanger){
              var div = document.getElementById('slider')
              div.innerHTML = '<img src="'+imgchanger+'" />';
          }
      </script>
  </body>
</html>